TextField {
  top: 0;
  left: 0;
  height: 20;
  width: 100%;
  margin: 2 5 5 5;
}

.manage-container {
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  clear-depth: true;

  .input-container {
    width: 90%;
    height: 165;
    left: 50%;
    top: 47%;
    alignment-x: center;
    alignment-y: center;

    background-color: rgba(20, 20, 20, 150);
    padding: 10;

    .input-list {
      left: 0;
      top: 0;
      width: 100%;
      bottom: 0;

      .input-wrapper {
        left: 0;
        width: 100%;
        height: 140;
      }
    }
  }
}

.settings-wrapper {
  left: 0;
  height: 100%;
  width: 67%;
  alignment-x: left;
  alignment-y: center;
}

.label-name {
  margin: 10 5 0 5;
}

.browse-skin {
  height: 20;
  margin: 5;
}

.checkbox-div {
  top: 0;
  left: 0;
  height: 50;
  width: 100%;
  alignment-x: center;
  alignment-y: center;
  margin: 10;
}

.checkbox-name {
  top: 0;
  alignment-x: center;
  alignment-y: center;
  font-size: 0.8;
}

.checkbox-item {
  top: 20;
  width: 20;
  height: 20;
  alignment-x: center;
  alignment-y: center;
}

.model-wrapper {
  priority-layer: very_back;
  height: 100%;
  width: 33%;
  top: 0;
  alignment-x: right;
  alignment-y: top;
  stencil: true;

  .model {
    left: 50%;
    top: 45%;
    height: 120;
    width: 60;
    alignment-x: center;
    alignment-y: center;
    interactable-outside: true;

    background-color-transition-duration: 150;
  }
}

.remove-container {
  width: 90%;
  height: fit-content;
  left: 50%;
  top: 50%;
  alignment-x: center;
  alignment-y: center;
  space-between-entries: 15;

	background-color: rgba(20, 20, 20, 150);
  padding: 10;
}

.remove-confirmation {
  left: 50%;
  alignment-x: center;
}

.remove-preview {
  left: 50%;
  alignment-x: center;
  height: 24;
  width: 100%;
}

.remove-button-menu {
  left: 50%;
  alignment-x: center;
  height: 20;
  layout: fill;
  space-between-entries: 8;
  width: 90%;
}